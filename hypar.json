{
  "$schema": "https://hypar.io/Schemas/Function.json",
  "id": "6bcf9d36-19d4-4713-8745-5d8a42ef8d61",
  "name": "MJ_ProceduralApartmentPlacer",
  "description": "Places an apartment mix in a procedurally generated mass.",
  "language": "C#",
  "model_dependencies": [
    {
      "autohide": false,
      "name": "Floors",
      "optional": false
    }
  ],
  "input_schema": {
    "type": "object",
    "properties": {
      "Unit Mix": {
        "$hyparCanPopOut": true,
        "default": {
          "Nodes": [
            {
              "Space Type": "Unit A",
              "Unit Count": 60,
              "Unit Area": 48,
              "Target Area": 48,
              "Color": {
                "Red": 0,
                "Alpha": 1,
                "Blue": 1,
                "Green": 0.415
              }
            },
            {
              "Space Type": "Unit B",
              "Unit Count": 60,
              "Unit Area": 62,
              "Target Area": 62,
              "Color": {
                "Red": 0,
                "Alpha": 1,
                "Blue": 0.517,
                "Green": 1
              }
            },
            {
              "Space Type": "Unit C",
              "Unit Count": 60,
              "Unit Area": 80,
              "Target Area": 80,
              "Color": {
                "Red": 0.733,
                "Alpha": 1,
                "Blue": 0.2,
                "Green": 1
              }
            }
          ]
        },
        "type": "object",
        "$hyparOrder": 5,
        "$hyparStyle": "matrix",
        "properties": {
          "Nodes": {
            "type": "array",
            "items": {
              "type": "object",
              "$hyparStyle": "row",
              "properties": {
                "Space Type": {
                  "type": [
                    "string",
                    "null"
                  ]
                },
                "Unit Count": {
                  "type": "number",
                  "$hyparUnitType": "none"
                },
                "Unit Area": {
                  "type": "number",
                  "$hyparUnitType": "area"
                },
                "Color": {
                  "description": "",
                  "$ref": "https://hypar.io/Schemas/Geometry/Color.json"
                }
              }
            },
            "default": [
              {
                "Space Type": "Unit A",
                "Unit Count": 60,
                "Unit Area": 48,
                "Target Area": 48,
                "Color": {
                  "Red": 0,
                  "Alpha": 1,
                  "Blue": 1,
                  "Green": 0.415
                }
              },
              {
                "Space Type": "Unit B",
                "Unit Count": 60,
                "Unit Area": 62,
                "Target Area": 62,
                "Color": {
                  "Red": 0,
                  "Alpha": 1,
                  "Blue": 0.517,
                  "Green": 1
                }
              },
              {
                "Space Type": "Unit C",
                "Unit Count": 60,
                "Unit Area": 80,
                "Target Area": 80,
                "Color": {
                  "Red": 0.733,
                  "Alpha": 1,
                  "Blue": 0.2,
                  "Green": 1
                }
              }
            ]
          }
        }
      },
      "Seam": {
        "multipleOf": 0.01,
        "default": 0.54,
        "maximum": 1,
        "description": "Parameter that can be adjusted for apartment stack placement (2d).",
        "type": "number",
        "$hyparOrder": 4,
        "minimum": 0
      },
      "CorePolygons": {
        "description": "Core polygons around which apartments should be placed.",
        "default": [],
        "optional": true,
        "type": "array",
        "$hyparOrder": 2,
        "items": {
          "name": "CorePolygons",
          "description": "A list of cores the apartments should be placed around.",
          "default": null,
          "type": "object",
          "$ref": "https://hypar.io/Schemas/Geometry/Polygon.json"
        }
      },
      "CellSize": {
        "multipleOf": 0.1,
        "default": 10,
        "maximum": 15,
        "description": "CellSize from MJ_ProceduralMass ",
        "type": "number",
        "$hyparOrder": 0,
        "minimum": 8
      }
    }
  },
  "outputs": [
    {
      "unit_type": "none",
      "name": "Units Placed",
      "description": "Apartment units placed.",
      "type": "number"
    },
    {
      "unit_type": "none",
      "name": "Units Remaining",
      "description": "Apartment units remaining/ not placed.",
      "type": "number"
    }
  ],
  "element_types": [
    "https://prod-api.hypar.io/schemas/Envelope",
    "https://raw.githubusercontent.com/hypar-io/Schemas/master/Room.json",
    "https://prod-api.hypar.io/schemas/ProgramType"
  ],
  "repository_url": "",
  "source_file_key": null,
  "preview_image": null
}